# Google Cloud Configuration for OAuth2 Authentication
GOOGLE_CLOUD_PROJECT=<your_google_cloud_project_id>

# Environment Configuration
ENVIRONMENT=development

# Optional: Service Account for server-to-server authentication
# GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account.json

# Optional: Setia module configuration
SETIA_RISK_DATA_BUCKET_NAME=<your_risk_data_bucket_name>

# Optional: Ollama configuration for Sarana module
OLLAMA_API_BASE_URL=http://localhost:11434

# Optional: API Server configuration
PORT=8080
HOST=0.0.0.0

# Optional: Logging configuration
LOG_LEVEL=INFO

# Legacy: If you still want to use API key method (not recommended for production)
# GEMINI_API_KEY=<your_gemini_api_key>
